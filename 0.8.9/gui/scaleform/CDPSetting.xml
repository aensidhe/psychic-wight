<?xml version="1.0" encoding="utf-8"?>
<damagePanel>
	
	<!-- Авторы: Shtys, STL1te -->
	
	<!-- Список возможных макросов:
	{{n}}               - переход на новую строку
	{{t}}				- фиксированный отступ (равнозначен TAB)
	{{hp}}              - текущее ХП
	{{hp-max}}          - максимальное ХП
	{{hp-ratio}}        - процент оставшегося ХП
	{{dmg}}             - общий урон, который нанесли танку
	{{dmg-ratio}}       - общий урон, который нанесли танку в процентах
	{{count}}           - порядковый номер нанесенного урона танку
	{{dmg-one}}         - урон, который нанесли танку одним попаданием
	{{dmg-ratio-one}}   - урон, который нанесли танку одним попаданием в процентах
	{{time-rel}}        - время с момента первого попадания по танку
	{{time-abs}}        - время попадания по игровому таймеру боя
	{{speed}}           - скорость движения танка
	{{player}}          - ник игрока
	{{vehicle}}         - название танка
	{{repair-100}}      - таймер ремонта модулей с округлением до сотых
	{{repair-10}}       - таймер ремонта модулей с округлением до десятых
	{{repair}}          - таймер ремонта модулей с округлением до целых в большую сторону
	{{atk-name-full}}	- полный ник атакующего
	{{atk-name-short}}	- краткий ник атакующего
	{{atk-veh-full}}	- полное название танка атакующего
	{{atk-veh-short}}	- краткое название танка атакующего
	{{atk-veh-type}}	- тип техники атакующего
	{{atk-veh-icon}}	- имя иконки техники атакующего
	{{atk-shell}}		- тип снаряда атакующего
	{{atk-dmg-kind}}	- тип урона
	Макросы динамического цвета:
	{{c:shellGold}}		- цвет по типу снаряда атакующего (серебро/золото)
	{{c:dmgKind}}		- цвет по типу урона атакующего
	{{c:vehType}}		- цвет по типу техники атакующего -->
	
	<!-- Настройка элементов боевого интерфейса -->
	<battle>
	
		<!-- Настройка панели повреждений -->
		<damagePanel>
			<location x="0" y="230" center="false"/> <!-- x, y - координаты расположения панели. Отсчет координат идет от левого нижнего угла экрана. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->			
		</damagePanel>
		
		<!-- Настройка панели снарядов -->
		<consumablesPanel>
			<location x="0" y="58" center="true"/> <!-- x, y - координаты расположения панели. Отсчет координат идет от левого нижнего угла экрана. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		</consumablesPanel>
		
		<!-- Настройка панели сообщения о выведенных из строя модулях и контуженного экипажа -->
		<vehicleMessagesPanel>
			<location x="-250" y="68" center="true"/> <!-- x, y - координаты расположения панели. Отсчет координат идет от левого нижнего угла экрана. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		</vehicleMessagesPanel>
		
		<!-- Настройка боевого чата -->
		<messenger>
			<location x="0" y="622" center="false"/> <!-- x, y - координаты расположения панели. Отсчет координат идет от левого нижнего угла экрана. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		</messenger>
		
	</battle>
	
	<!-- Настройка фона панели -->
	<background alpha="80"> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		<location x="0" y="0" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<scale x="100" y="100"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
	</background>
	
	<!-- Настройка индикатора танка -->
	<tankIndicator>
	
		<!-- Настройка индикатора танка -->
		<tank alpha="100"> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
			<location x="115" y="130" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
			<filter size="3" angle="0" distance="0" color="0x000000" alpha="0" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
			<scale x="75" y="75"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
		</tank>
		
		<!-- Настройка орудия индикатора танка -->
		<gun alpha="100"/> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		
		<!-- Настройка башни индикатора танка -->
		<turret alpha="100"/> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		
		<!-- Настройка корпуса индикатора танка -->
		<hull alpha="100"/> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		
		<!-- Настройка Углов Горизонтальной Наводки индикатора танка -->
		<gunConstraints alpha="100"> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
			<color normal="0x999999" critical="0xFF0000"/> <!-- normal - цвет нормального УГН. critical - цвет граничного УГН -->
		</gunConstraints>
	
	</tankIndicator>
	
	<!-- Настройка экипажа -->
	<crew alpha="75"> <!-- alpha - прозрачность не контуженного экипажа. 100 - непрозрачно, 0 - прозрачно -->
		<location x="230" y="202"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. Для расположения экипажа в центре, нужно указывать х - равный ширине изображения экрана -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="0" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<color normal="0x999999" critical="0xFF6600"/> <!-- normal - цвет нормального экипажа. critical - цвет контуженного экипажа -->
		<scale x="100" y="100"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
	</crew>
	
	<!-- Настройка иконки фиксации корпуса -->
	<lockIcon> 
		<location x="142" y="52" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<filter size="3" angle="0" distance="0" color="0xFF0000" alpha="100" strength="80"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<scale x="100" y="100"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
		<color color="0xFF0000"/> <!-- color - цвет иконки -->
	</lockIcon>
	
	<!-- Настройка круиз-контроля -->
	<cruiseCtrl alpha="100"> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		<location x="36" y="80" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<scale x="100" y="100"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
	</cruiseCtrl>
	
	<!-- Настройка полоски ХП -->
	<healthBar alpha="100"> <!-- alpha - прозрачность. 100 - непрозрачно, 0 - прозрачно -->
		<location x="1" y="22" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<scale x="100" y="100"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
		<color start="0xFD0100" middle="0xFF6700" end="0x009000"/> <!-- start - цвет полоски когда ХП = 0%. middle - цвет полоски когда ХП = 50%. end - цвет полоски когда ХП = 100%. middle можно убрать, тогда цвет полоски при ХП = 50% посчитается автоматически -->
	</healthBar>
	
	<!-- Настройка ХП -->
	<healthField visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="23" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="230" height="25"/>  <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<font face='Cuprum' size='14' color='#FFFFFF'><p align='center'>{{hp}} / {{hp-max}}</p></font>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</healthField>
	
	<!-- Настройка журнала полученного урона -->
	<damageLog visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="235" y="3" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="450" height="226"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<textformat leading='5' tabstops='[30,70,100,180]'><b><font face='Cuprum' size='15' color='#FFFFFF'>{{count}}:<tab><font color='{{c:dmgKind}}'>-{{dmg-one}}</font><tab><font color='{{c:shellGold}}'>{{atk-shell}}</font><tab><font color='{{c:vehType}}'>{{atk-veh-short}}</font><tab>{{atk-name-short}}</font></b></textformat>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
		<direction>down</direction> <!-- Направление записи в журнал. up - последний урон всегда вверху, down - последний урон всегда внизу -->
	</damageLog>
	
	<!-- Настройка всплывающего сообщения о последнем полученном уроне -->
	<damageHit visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="350" y="-80" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="450" height="60"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<b><font face='Cuprum' size='20' color='#FFFFFF'><font color='{{c:dmgKind}}'>-{{dmg-one}}</font>{{t}}<font color='{{c:vehType}}'>{{atk-veh-short}}</font>{{t}}<font color='{{c:shellGold}}'>{{atk-shell}}</font></font></b>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
		<delay>7</delay> <!-- Время, после которого сообщение исчезнет -->
	</damageHit>
	
	<!-- Настройка сообщения о пожаре -->
	<fireMessage enabled="true"> <!-- enabled - видимость элемента, true/false - включено/выключено -->
		<location x="250" y="182" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="230" height="70"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="100"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<font face='Cuprum' size='16' color='#FF0000'><p align='center'>Танк горит!</p></font>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</fireMessage>
	
	<!-- Настройка иконки пожара -->
	<fireIcon visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
        <location x="115" y="65" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
        <filter size="3" angle="0" distance="0" color="0x000000" alpha="0" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
        <scale x="75" y="75"/> <!-- x, y - масштаб элемента в процентах. 100 - оригинальный размер -->
        <color color="0xFF0000"/> <!-- color - цвет иконки -->
	</fireIcon>
		
	<!-- Настройка скорости -->
	<speedField visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="62" y="54" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="100" height="25"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<textformat tabstops='18'><font face='Cuprum' size='15' color='#FFFFFF'><p align='left'>{{speed}}<tab><font size='10'>km/h</font></p></font></textformat>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</speedField>
	
	<!-- Настройка таймера ремонта модулей -->
	<repairTimer visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="0" y="7"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла модуля -->
		<text alpha="100" width="45" height="35"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[{{repair}}]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</repairTimer>
	
	<!-- Настройка имени игрока -->
	<playerName visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="4" y="3" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="110" height="25"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<font face='Cuprum' size='14' color='#FFFFFF'><p align='left'>{{player}}</p></font>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</playerName>
	
	<!-- Настройка названия танка -->
	<vehicleName visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="95" y="3" center="false"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от верхнего левого угла панели. center - включение отсчета x координаты от центра экрана, true/false - включено/выключено -->
		<text alpha="100" width="130" height="25"/> <!-- alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format><![CDATA[<font face='Cuprum' size='14' color='#FFFFFF'><p align='right'>{{vehicle}}</p></font>]]></format> <!-- HTML-строка. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</vehicleName>

	<!-- Настройка цветов -->
	<colors>
		<shellGold credits="#FFFFFF" gold="#FF6655"/> <!-- Цвет по типу снаряда. credits - кредиты. gold - золото -->
