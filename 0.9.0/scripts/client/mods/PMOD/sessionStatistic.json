/*
	Сессионная статистика
	Session statistic
*/
{
	// true - включить функцию.
	// true - enable function.
	"enable": true,
	
	// Автоматически обнулять статистику по истечению срока годности.
	// Automatically reset the statistics after the expiration date.
	"autoReset": true,
	
	// Количество дней на протяжении которых хранится статистика.
	// The number of days during which the stored statistics.
	"daysToBeSaved": 1,

	// Смещение времени относительно вашего часового пояса в стандарте UTC.
	// Time offset of your time zone in the UTC standard.
	"dateUTCoffset": 0,

	// Сообщение на случай отсутствия проведенных боев за сессию.
	// Message in case of absence spent battles for session.
	"needBattleToProcess": "<font color='#EEEEEE'> \n <b>PMOD - Сессионная статистика</b></font> \n\n{{helloMessage}}, для отображения изменений в статистике нужно провести хотя бы один бой!",
	
	// true - Обрабатывать бои которые идут в основную статистику
	// false - Обрабатывать все бои без исключений		
	// true - Process battles that go to general statistics
	// false - Process all battles without exception
	"processOnlyStatBattles": true,
	
	// Подложка сообщения
	// Message background
	"messageBackground": {
		
		// true - Использовать подложку
		// true - Use a substrate
		"enable": false,
		
		// Путь к изображению
		// Path to image
		"path": "../maps/icons/library/BgBattleResultIconVictory-1.png"
	},
	
	// Иконка сообщения
	// Message icon
	"messageIcon": {

		// true - Использовать иконку
		// true - Use a icon
		"enable" : false,
		
		// Путь к изображению
		// Path to image
		"path": "../maps/icons/library/PersonalAchievementsIcon-1.png"
	},
	
	// Кнопка обнуления статистики
	// Reset statistic button 
	"resetButton": {
	
		// true - Отображать кнопку
		// true - Display button
		"enable": true,
		
		// Надпись на кнопке
		// Button text
		"buttonText": "Обнулить статистику"
	},

	// Заготовка сообщения. Список макросов смотрите в файле readme.
	// The blank message. list of macroses watch in readme file.
	"statMessege": [
		"<font color='#EEEEEE'>\n<b><font size='17'>PMOD - Сессионная статистика</font></b></font>\n\n\n",
		"<font color='#EEEEEE'><b><font size='15'>Показатели за сессию:</font></b></font>\n<textformat  tabstops='[85,110,165]'>\n",
		"<font color='#EEEEEE'>Побед / Боев:</font><tab><tab><b>{{Wins}} / {{Battles}}</b><tab>(<font color='{{c:WinRate}}'><b>{{WinRate}}%</b></font>)\n",
		"<font color='#EEEEEE'>Урон <font size='10'>(средн)</font>: </font><tab><tab><b>{{DamageDealt}}</b><tab>(<font color='{{c:AvgDamageDealt}}'><b>{{AvgDamageDealt}}</b></font>)\n",
		"<font color='#EEEEEE'>Уничтожено: </font><tab><tab><b>{{Frags}}</b><tab>(<font color='{{c:AvgFrags}}'><b>{{AvgFrags}}</b></font>)\n",
		"<font color='#EEEEEE'>Обнаружено:</font><tab><tab><b>{{SpottedEnemies}}</b><tab>(<font color='{{c:AvgSpottedEnemies}}'><b>{{AvgSpottedEnemies}}</b></font>)\n",
		"<font color='#EEEEEE'>Опыт <font size='10'>(средн)</font>: </font><tab> <tab><b>{{BattleXP}}</b><tab>(<font color='{{c:AvgBattleXP}}'><b>{{AvgBattleXP}}</b></font>)\n\n",
		"<font color='#EEEEEE'><b><font size='15'>Рейтинги за сессию:</font></b></font>\n\n",
		"<font color='#EEEEEE'>WN8 <font size='10'>(xwn8)</font>:<tab><tab><font color='{{c:WN8}}'><b>{{WN8}}</b></font><tab>(<font color='{{c:XWN8}}'><b>{{XWN8}}</b></font>)</font>\n",
		"<font color='#EEEEEE'>WN7 <font size='10'>(xwn7)</font>:<tab><tab><font color='{{c:WN7}}'><b>{{WN7}}</b></font><tab>(<font color='{{c:XWN7}}'><b>{{XWN7}}</b></font>)</font>\n",
		"<font color='#EEEEEE'>WN6 <font size='10'>(xwn6)</font>:<tab><tab><font color='{{c:WN6}}'><b>{{WN6}}</b></font><tab>(<font color='{{c:XWN6}}'><b>{{XWN6}}</b></font>)</font>\n",
		"<font color='#EEEEEE'>EFF (РЭ)<font size='10'>(xeff)</font>:<tab><tab><font color='{{c:EFF}}'><b>{{EFF}}</b></font><tab>(<font color='{{c:XEFF}}'><b>{{XEFF}}</b></font>)</font>\n\n",
		"<font color='#EEEEEE'><b><font size='15'>Влияние сессии на аккаунт:</font></b></font>\n\n<textformat  tabstops='[85,110,155]'>",
		"<font color='#EEEEEE'>Процент побед: </font><tab><tab><tab><font color='{{c:DiffWinrate}}'><b>{{DiffWinrate}}</b></font>\n",
		"<font color='#EEEEEE'>Средний урон: </font><tab><tab><tab><font color='{{c:DiffAvgDamageDealt}}'><b>{{DiffAvgDamageDealt}}</b></font>\n",
		"<font color='#EEEEEE'>В среднем уничтожено: </font><tab><tab><tab><font color='{{c:DiffAvgFrags}}'><b>{{DiffAvgFrags}}</b></font>\n",
		"<font color='#EEEEEE'>В среднем обнаружено: </font><tab><tab><tab><font color='{{c:DiffAvgSpottedEnemies}}'><b>{{DiffAvgSpottedEnemies}}</b></font>\n",
		"<font color='#EEEEEE'>Средний опыт: </font><tab><tab><tab><font color='{{c:DiffAvgXP}}'><b>{{DiffAvgXP}}</b></font>\n<textformat  tabstops='[85,110,165]'>",
		"<font color='#EEEEEE'>Заработано <font size='10'>(средн)</font>: </font><tab> <tab><b>{{BattleCredits}}</b><tab>(<font color='{{c:AvgBattleCredits}}'><b>{{AvgBattleCredits}}</b></font>)\n",
		"<font color='#EEEEEE'>Доход <font size='10'>(средн)</font>:</font><tab> <tab><b>{{BattlePureCredits}}</b><tab>(<font color='{{c:AvgBattlePureCredits}}'><b>{{AvgBattlePureCredits}}</b></font>)\n"
	],

